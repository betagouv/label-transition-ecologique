{
  "kind": "singleType",
  "collectionName": "page_contacts",
  "info": {
    "singularName": "page-contact",
    "pluralName": "page-contacts",
    "displayName": "4. Contact",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "titre": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "default": "Contacter l'équipe"
    },
    "description": {
      "type": "text",
      "default": "Des questions sur le programme ou sur la plateforme ? Écrivez-nous et nous vous répondrons dès que possible."
    },
    "couverture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "telephone": {
      "type": "string",
      "default": "04 15 09 82 07",
      "required": true
    },
    "horaires": {
      "type": "string",
      "default": "Permanence de 9h à 12h30 et de 14h à 16h30, du lundi au vendredi",
      "required": true,
      "maxLength": 255
    },
    "legende_visible": {
      "type": "boolean",
      "default": false
    }
  }
}
